    <!-- Main content -->
  
        <!-- /.card-header -->
        <div class="card-body">
          <div id="jsGrid1">
            <table  class="table table-striped">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                  <th scope="col">Budget</th>
                  <th scope="col">Message</th>
                  <th scope="col">Checklist</th>
                  <th scope="col">Hireteam</th>
               
                  </tr>
                </thead>
                <tbody>
                  <% for(var i = 0; i < data.length; i++) { %>
                        <tr>
                          <td id="ids"><%= i + 1 %></td>
                          <td><%= data[i].name %></td>
                 
                          <td><%= data[i].email %></td>
                          <td><%= data[i].phone %></td>
                          <td><%= data[i].budget %></td>
                          <td><%= data[i].message %></td>
                          <td><%= data[i].checklist %></td>
                          <td><%= data[i].hireteam %></td>
                        
                        </tr>
    
                        <% } %> 
                 
                </tbody>
              </table>
          </div>
        </div>
    
  <!-- /.content-wrapper -->

  <script src="/public/js/index.js"></script>

