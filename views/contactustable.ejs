          <div id="jsGrid1">
            <table  class="table table-striped" id="dataTable">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                  <th scope="col">Budget</th>
                  <th scope="col">Message</th>
                  <th scope="col">Checklist</th>
                  <th scope="col">Hireteam</th>
               
                  </tr>
                </thead>
                <tbody>
                 <% for(var i = 0; i < data.length; i++) { %>
                        <tr>
                          <td id="ids"><%= i + 1 %></td>
                          <td><%= data[i].name %></td>
                 
                          <td><%= data[i].email %></td>
                          <td><%= data[i].phone %></td>
                          <td><%= data[i].budget %></td>
                          <td><%= data[i].message %></td>
                   
                        
                        <td>
                          <% for(let index = 0; index < data[i].checklist.length; index++) { %>
                          <li style="list-style: none;">  <%=data[i].checklist[index]  %></li>
                          <% } %> 
                        </td>
                      
                         
                          
                         <td>
                          <% for(let index = 0; index < data[i].hireteam.length; index++) { %>
                           <li style="list-style: none;"><%= data[i].hireteam[index] %></li> 
                          <% } %> 
                        </td> 
                        </tr>
    
                        <% } %>
                
                </tbody>
              </table>
