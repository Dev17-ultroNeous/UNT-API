  <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <section class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">

            </div>
            <div class="col-sm-6">
              <!-- <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item" style="background-color: #007bff; color: aliceblue; margin-top: 44px;
              padding-right: 10px;
              padding-left: 10px;"><a href="employefeedbackadd" style="color: aliceblue;"><i class="fa fa-plus" aria-hidden="true"></i>Add</a></li>
       
            </ol> -->
            </div>
          </div>
        </div><!-- /.container-fluid -->
      </section>

      <!-- Main content -->
      <!-- /.card-header -->
      <div class="card-body">
        <div id="jsGrid1">
          <table class="table table-striped" id="dataTable">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Phone</th>
                <th scope="col">Budget</th>
                <th scope="col">Message</th>
                <th scope="col">Checklist</th>
                <th scope="col">Hireteam</th>

              </tr>
            </thead>
            <tbody>
              <% for(var i=0; i < data.length; i++) { %>
                <tr>
                  <td id="ids">
                    <%= i + 1 %>
                  </td>
                  <td>
                    <%= data[i].name %>
                  </td>
                  <td>
                    <%= data[i].email %>
                  </td>
                  <td>
                    <%= data[i].phone %>
                  </td>
                  <td>
                    <%= data[i].budget %>
                  </td>
                  <td> <a class="btn btn-sm rounded-0" data-toggle="modal" data-target="#exampleModal" id="edit"
                      data-toggle="tooltip" data-placement="top" title="View" message="<%= data[i].message %>"><i
                        class="fa fa-eye" aria-hidden="true"></i></a> </td>
                  <td>
                    <a class="btn btn-sm rounded-0" data-toggle="modal" data-target="#exampleModal" id="edit"
                      data-toggle="tooltip" data-placement="top" title="View" message="<% for(let index = 0; index < data[i].checklist.length; index++) { %>
                         <%= data[i].checklist[index] %>
                          <% } %>"><i class="fa fa-eye" aria-hidden="true"></i></a>

                  </td>
                  <td>

                    <a class="btn btn-sm rounded-0" data-toggle="modal" data-target="#exampleModal" id="edit"
                      data-toggle="tooltip" data-placement="top" title="View" message="<% for(let index = 0; index < data[i].hireteam.length; index++) { %>
                         <%= data[i].hireteam[index] %>
                          <% } %>"><i class="fa fa-eye" aria-hidden="true"></i></a>
                  </td>
                </tr>
                <% } %>

            </tbody>
          </table>

        </div>
      </div>
      <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
          <div class="modal-content">

            <div class="modal-body">
              <textarea id="message" rows="8" cols="50"></textarea>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>

            </div>
          </div>
        </div>
      </div>
      <!-- /.content-wrapper -->

      <script src="/public/js/index.js"></script>

      <!-- jQuery -->
      <script src="../../plugins/jquery/jquery.min.js"></script>
      <!-- Bootstrap 4 -->
      <script src="../../plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
      <!-- jsGrid -->
      <script src="../../plugins/jsgrid/demos/db.js"></script>
      <script src="../../plugins/jsgrid/jsgrid.min.js"></script>
      <!-- AdminLTE App -->
      <script src="../../dist/js/adminlte.min.js"></script>
      <!-- AdminLTE for demo purposes -->
      <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.27.0/axios.min.js"></script>
      <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
      <!-- Page specific script -->
      <script>

        localStorage.setItem("authorized", "ultroneous");
        $(document).on("click", "#edit", async function () {

          let message = $(this).attr("message")

          document.getElementById('message').value = `${message}`
        })


  jQuery(function ($) {
        var path = window.location.href; // because the 'href' property of the DOM element is the absolute path
        $('li a').each(function () {
          if (this.href === "http://localhost:8000/admin/contactustable") {
            $(this).addClass('active');
          }
        });
      });
      </script>
</body>

